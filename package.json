{
  "name": "@liquid-labs/validators",
  "version": "0.0.3",
  "description": "Validators for user input (strings).",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint *.js --ignore-pattern '.*'",
    "build": "babel --source-maps inline *.js -d dist",
    "start": "babel --watch --source-maps inline *.js -d dist",
    "prepare": "npm run lint",
    "prepublishOnly": "rm -rf dist && babel *.js -d dist && find dist -name '*.test.js' | xargs rm",
    "pretest": "babel --source-maps inline *.js -d dist",
    "test": "jest --coverage dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Liquid-Labs/validators.git"
  },
  "keywords": [
    "validation"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liquid-Labs/validators/issues"
  },
  "homepage": "https://github.com/Liquid-Labs/validators#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.6.1",
    "jest": "^23.6.0"
  },
  "dependencies": {
    "@liquid-labs/regex-repo": "0.0.6"
  },
  "peerDependencies": {
    "core-js": "1.x || 2.x"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
